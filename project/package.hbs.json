{
  "devDependencies": {
    "@mblabs/jest-preset": "^1.0.0"
  },
  "eslintConfig": {
    "rules": {
      "woke/all": "off"
    }
  },
  "files": ["dist", "CHANGELOG.md", "LICENSE", "README.md"],
  "jest": {
    "preset": "@mblabs/jest-preset",
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "filename": "index.html",
          "logoImgPath": "./.config/assets/logo-jest-report.png",
          "openReport": true,
          "pageTitle": "Code Coverage Report",
          "publicPath": "./coverage"
        }
      ]
    ]
  },
  "keywords": ["angular", "chrome", "extension", "firefox", "electron", "android", "ios", "pwa", "rxjs"],
  "main": "dist/main.js",
  "typings": "dist/main.d.ts"
}
