{
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.0",
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@mblabs/jest-preset": "^1.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^12.11.1",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "typescript": "~4.4.3"
  },
  "eslintConfig": {
    "rules": {
      "woke/all": "off"
    }
  },
  "files": ["dist", "CHANGELOG.md", "LICENSE", "README.md"],
  "jest": {
    "preset": "@mblabs/jest-preset",
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "filename": "index.html",
          "logoImgPath": "./.config/assets/logo-jest-report.png",
          "openReport": true,
          "pageTitle": "Code Coverage Report",
          "publicPath": "./coverage"
        }
      ]
    ]
  },
  "keywords": ["angular", "chrome", "extension", "firefox", "electron", "android", "ios", "pwa", "rxjs"],
  "main": "dist/main.js",
  "scripts": {
    "build": "ng build",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test:cov": "ng test --coverage",
    "watch": "ng build --watch --configuration development"
  },
  "typings": "dist/main.d.ts"
}
