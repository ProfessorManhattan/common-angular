{
  "devDependencies": {
    "@megabytelabs/jest-preset": "^1.0.0"
  },
  "files": ["dist", "CHANGELOG.md", "LICENSE", "README.md"],
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "jest": {
    "preset": "@megabytelabs/jest-preset",
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "filename": "index.html",
          "logoImgPath": "./.config/assets/logo-icon.png",
          "openReport": true,
          "pageTitle": "Code Coverage Report",
          "publicPath": "./coverage"
        }
      ]
    ]
  },
  "keywords": [
    "angular",
    "chrome",
    "extension",
    "firefox",
    "electron",
    "android",
    "ios",
    "pwa",
    "rxjs"
  ],
}
