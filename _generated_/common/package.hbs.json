{
  "author": "{{author.name}} <{{author.email}}> ({{link.home}})",
  "blueprint": {
    "description": "An NPM package that is part of the Megabyte Labs eco-system",
    "name": "My Project Name",
    "overview": "This text shows up at the top of the README.md.",
    "repository": {
      "github": "{{#if repository.github}}{{repository.github}}{{else}}https://github.com/{{profile.github}}/{{repository.prefix.github}}{{slug}}{{/if}}",
      "gitlab": "{{#if repository.gitlab}}{{repository.gitlab}}{{else}}{{repository.gitlabBaseUrl}}/{{group}}/{{subgroup}}/{{slug}}{{/if}}"
    },
    "slug": "my-project",
    "title": "Title of the README.md"
  },
  "bugs": {
    "email": "{{email.help}}",
    "url": "{{#if repository.gitlab}}{{repository.gitlab}}{{else}}{{repository.gitlabBaseUrl}}/{{group}}/{{subgroup}}/{{slug}}{{/if}}{{repository.location.issues.gitlab}}"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "description": "{{description}}",
  "devDependencies": {
    "@angular-builders/jest": "^13.0.0",
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@mblabs/jest-preset": "^1.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "commitlint-config-gitmoji": "^2.2.5",
    "glob": "^7.2.0",
    "handlebars-helpers": "^0.10.0",
    "jest": "^27.3.1",
    "semantic-release": "^18.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "~4.4.3"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "eslintConfig": {
    "extends": "@mblabs/eslint-config",
    "rules": {
      "woke/all": "off"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "funding": [
    {
      "type": "opencollective",
      "url": "{{profile_link.opencollective}}/{{profile.opencollective}}"
    },
    {
      "type": "patreon",
      "url": "{{profile_link.patreon}}/{{profile.patreon}}"
    }
  ],
  "homepage": "{{link.home}}",
  "jest": {
    "preset": "@mblabs/jest-preset",
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "filename": "index.html",
          "logoImgPath": "./.config/assets/logo-jest-report.png",
          "openReport": true,
          "pageTitle": "Code Coverage Report",
          "publicPath": "./coverage"
        }
      ]
    ]
  },
  "keywords": [
    "android",
    "angular",
    "chrome",
    "electron",
    "extension",
    "firefox",
    "ios",
    "mblabs",
    "megabytelabs",
    "professormanhattan",
    "pwa",
    "rxjs"
  ],
  "license": "{{license}}",
  "lint-staged": {
    "**/*": [
      "blocklint --wordlist blacklist,slave,whitelist",
      "check-merge-conflict",
      "check-added-large-files",
      "check-symlinks",
      "destroyed-symlinks"
    ],
    "**/*.(css|html|less|md|mjml|php|scss|sh|xml)": [
      "task fix:prettier --"
    ],
    "**/*.(jpg|png)": [
      "task image:compress --"
    ],
    "**/*.(js|jsx|ts|tsx)": [
      "task fix:js --",
      "task lint:js --"
    ],
    "**/*.json": [
      "task fix:json --"
    ],
    "**/*.sh": [
      "task fix:shell --",
      "task lint:shell --"
    ],
    "**/*.toml": [
      "task lint:toml --"
    ],
    "**/*.xml": [
      "task fix:xml --",
      "task lint:xml --"
    ],
    "**/*.yml": [
      "task fix:yaml --",
      "task lint:yaml --"
    ],
    "logo.png": [
      "task image:resize -- 200 200",
      "task image:compress --"
    ]
  },
  "main": "dist/main.js",
  "name": "{{#if customPackageName}}{{customPackageName}}{{else}}@{{profile.npmjs_organization}}/{{repository.prefix.github}}{{slug}}{{/if}}",
  "optionalDependencies": {
    "chalk": "^4.1.2",
    "inquirer": "^8.2.0",
    "signale": "^1.4.0",
    "string-break": "^1.2.0"
  },
  "prettier": "@mblabs/prettier-config",
  "private": true,
  "publishConfig": {
    "access": "{{npm_publish_config_access}}"
  },
  "release": {
    "branches": [
      "master",
      "next"
    ],
    "extends": "@mblabs/release-config"
  },
  "repository": {
    "type": "git",
    "url": "git+{{#if repository.github}}{{repository.github}}{{else}}https://github.com/{{profile.github}}/{{repository.prefix.github}}{{slug}}{{/if}}.git"
  },
  "scripts": {
    "build": "ng build",
    "help": "{{npm_scripts_help}}",
    "ng": "ng",
    "prepare": "{{npm_scripts_prepare}}",
    "repair": "bash <(curl -sS https://install.doctor/repair)",
    "start": "ng serve",
    "test": "ng test",
    "test:cov": "ng test --coverage",
    "watch": "ng build --watch --configuration development"
  },
  "type": "{{npm_type}}",
  "typings": "dist/main.d.ts",
  "version": "{{version}}"
}
